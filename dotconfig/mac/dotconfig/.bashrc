source /etc/bash_completion

alias cd..='cd ..'
alias c.='cd ..'
alias c..='cd ../..'
alias c...='cd ../../..'
alias c....='cd ../../../..'

alias gti='git'
alias du='du -hs'
alias ls='ls --color -h'
alias ks='ls --color -h'
alias tree='tree -ahC'
alias bc='bc -l'
alias jdgui='java -jar /opt/jd-gui/jd-gui.jar'

shopt -s nocaseglob #make ls *ttl* expand to TTL Cook. To unset use shopt -u nocaseglob


